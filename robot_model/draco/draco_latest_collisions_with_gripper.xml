      <!--<accelerometer name="imu-linear-acceleration" site="imu" noise="1e-4" cutoff="157"/>-->
      <magnetometer name="imu-magnetometer" site="imu"/>
  </sensor>

  <!--start with the fixed base pose-->
  <equality>
      <weld body1="world_link" body2="torso_link" relpose="0 0 0.95 1 0 0 0"/>

      <!--gripper-->
      <tendon tendon1="l_gripper_tendon_left" tendon2="l_gripper_tendon_right" polycoef="0 1 0 0 0"/>
      <joint joint1="left_ezgripper_knuckle_palm_L1_2" joint2="left_ezgripper_knuckle_palm_L1_1" polycoef="0 1 0 0 0"/>
      <tendon tendon1="r_gripper_tendon_left" tendon2="r_gripper_tendon_right" polycoef="0 1 0 0 0"/>
      <joint joint1="right_ezgripper_knuckle_palm_L1_2" joint2="right_ezgripper_knuckle_palm_L1_1" polycoef="0 1 0 0 0"/>
  </equality>

  <!--rolling contact mechanism-->
  <equality>
      <joint joint1="l_knee_fe_jp" joint2="l_knee_fe_jd" polycoef="0 1 0 0 0"/>
      <joint joint1="r_knee_fe_jp" joint2="r_knee_fe_jd" polycoef="0 1 0 0 0"/>
  </equality>

  <tendon>
    <spatial width="0.002" material="tendon_line" name="l_gripper_tendon_left" 
      limited="true" range="0 0.5" springlength="0" frictionloss="0.1" stiffness="0.4">
      <site site="l_gripper_site_left_finger1"/>
      <site site="l_gripper_site_left_finger2"/>
      <site site="l_gripper_site_left_finger3"/>
      <site site="l_gripper_site_left_finger4"/>
    </spatial>

    <spatial width="0.002" material="tendon_line" name="l_gripper_tendon_right" 
      limited="true" range="0 0.5" springlength="0" frictionloss="0.1" stiffness="0.4">
      <site site="l_gripper_site_right_finger1"/>
      <site site="l_gripper_site_right_finger2"/>
      <site site="l_gripper_site_right_finger3"/>
      <site site="l_gripper_site_right_finger4"/>
    </spatial>

    <spatial width="0.002" material="tendon_line" name="r_gripper_tendon_left" 
      limited="true" range="0 0.5" springlength="0" frictionloss="0.1" stiffness="0.4">
      <site site="r_gripper_site_left_finger1"/>
      <site site="r_gripper_site_left_finger2"/>
      <site site="r_gripper_site_left_finger3"/>
