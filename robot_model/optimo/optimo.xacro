<?xml version="1.0"?>
<robot name="optimo"
    xmlns:xacro="http://www.ros.org/wiki/xacro">

    <material name="white">
        <color rgba="1.0 1.0 1.0 1.0"/>
    </material>

    <material name="blue">
        <color rgba="0.5 0.5 1 1.0"/>
    </material>

    <!-- Used for fixing robot to Gazebo 'base_link' -->
    <link name="world" />

    <joint name="world_to_stand" type="fixed">
        <parent link="world"/>
        <child link="stand"/>
        <origin xyz="0 0 1" rpy="0 0 0"/>
    </joint>

    <link name="stand">
        <visual>
            <origin xyz="0 0 -0.5" rpy="0 0 0"/>
            <geometry>
                <box size="0.3 0.3 1"/>
            </geometry>
            <material name="white"/>
        </visual>
        <collision>
            <origin xyz="0 0 -0.5" rpy="0 0 0"/>
            <geometry>
                <box size="0.3 0.3 1"/>
            </geometry>
        </collision>
    </link>

    <joint name="stand_to_link0" type="fixed">
        <parent link="stand" />
        <child link="link0" />
        <!-- .27 puts the link on the stand -->
        <origin xyz="0 0 .27" rpy="0 0 0" />
    </joint>

    <!-- Base Link -->
    <link name="link0">
        <visual>
            <geometry>
                <mesh filename="package://meshes/link0.stl" />
            </geometry>
            <material name="blue"/>
        </visual>
        <collision>
            <geometry>
                <mesh filename="package://meshes/col0.stl" />
            </geometry>
        </collision>
        <inertial>            <!-- Doesn't matter, just placeholder values -->
            <origin xyz="0 0 0" />
            <mass value="1" />
            <inertia ixx="0" ixy="0.0" ixz="0.0" iyy="0" iyz="0.0" izz="0" />
        </inertial>
    </link>
    <link name="link1" />
    <joint name="joint1" type="revolute">
        <origin xyz="0 0 0" rpy="0 0 0" />
        <parent link="link0" />
        <child link="link1" />
        <axis xyz="0 0 1" />
        <limit lower="-2.792526902" upper="2.792526902" effort="79" velocity="10" />
        <dynamics damping="1" friction="0.1"/>
    </joint>
    <joint name="joint1_passive" type="fixed">
        <origin xyz="0 0 0" rpy="1.57 0 0" />j
        <parent link="link1" />
        <child link="link1_passive" />
    </joint>
    <link name="link1_passive">
        <visual>
            <geometry>
                <mesh filename="package://meshes/link1.stl" />
            </geometry>
            <material name="white"/>
        </visual>
        <collision>
            <geometry>
                <mesh filename="package://meshes/col1.stl" />
            </geometry>
        </collision>
        <inertial>
            <origin xyz="0 -0.01635 0.01549" />
            <mass value="4.152" />
            <inertia ixx="0.01" ixy="0.000017" ixz="-.00001" iyy="0.007494" iyz="0.000213" izz="0.009939" />
        </inertial>
    </link>
    <link name="link2" />
    <joint name="joint2" type="revolute">
        <origin xyz="0 0 0" rpy="0 0 0" />
        <parent link="link1_passive" />
        <child link="link2" />
        <axis xyz="0 0 1" />
        <limit lower="1.134464013" upper="4.188790202" effort="95" velocity="10" />
        <dynamics damping="1" friction="0.1"/>
    </joint>
    <joint name="joint2_passive" type="fixed">
        <origin xyz="0 0 0" rpy="1.57 0 0" />
        <parent link="link2" />
        <child link="link2_passive" />
    </joint>
    <link name="link2_passive">
        <visual>
            <geometry>
                <mesh filename="package://meshes/link2.stl" />
            </geometry>
            <material name="blue"/>
        </visual>
        <collision>
            <geometry>
                <mesh filename="package://meshes/col2.stl" />
            </geometry>
        </collision>
        <inertial>
            <origin xyz="0 -0.05063 0.05677" />
            <mass value="1.72" />
            <inertia ixx="0.005694" ixy="0" ixz="0" iyy="0.005902" iyz="0.001353" izz="0.002415" />
        </inertial>
    </link>
    <link name="link3" />
    <joint name="joint3" type="revolute">
        <origin xyz="0 0 0.4" rpy="0 0 0" />
        <parent link="link2_passive" />
        <child link="link3" />
        <axis xyz="0 0 1" />
        <limit lower="-2.792526902" upper="2.792526902" effort="32" velocity="10" />
        <dynamics damping="1" friction="0.1"/>
    </joint>
    <joint name="joint3_passive" type="fixed">
        <origin xyz="0 0 0" rpy="-1.57 0 0" />
        <parent link="link3" />
        <child link="link3_passive" />
    </joint>
    <link name="link3_passive">
        <visual>
            <geometry>
                <mesh filename="package://meshes/link3.stl" />
            </geometry>
            <material name="white"/>
        </visual>
        <collision>
            <geometry>
                <mesh filename="package://meshes/col3.stl" />
            </geometry>
        </collision>
        <inertial>            <!-- Inertia values taken from yaml, but these seem like they cannot be
            accurate...-->
            <origin xyz="-0.000039 .098 0" />
            <mass value="4.310" />
            <inertia ixx="0.012989" ixy="-0.000015" ixz="-.000003" iyy="0.002584" iyz="0.000632" izz="0.01305" />
        </inertial>
    </link>
    <link name="link4" />
    <joint name="joint4" type="revolute">
        <origin xyz="0 0 0" rpy="0 0 0" />
        <parent link="link3_passive" />
        <child link="link4" />
        <axis xyz="0 0 1" />
        <limit lower="-2.443460951" upper="2.443460951" effort="40" velocity="10" />
        <dynamics damping="1" friction="0.1"/>
    </joint>
    <joint name="joint4_passive" type="fixed">
        <origin xyz="0 0 0" rpy="1.57 0 0" />
        <parent link="link4" />
        <child link="link4_passive" />
    </joint>
    <link name="link4_passive">
        <visual>
            <geometry>
                <mesh filename="package://meshes/link4.stl" />
            </geometry>
            <material name="blue"/>
        </visual>
        <collision>
            <geometry>
                <mesh filename="package://meshes/col4.stl" />
            </geometry>
        </collision>
        <inertial>
            <origin xyz="-.00001 0.048852 0.0688" />
            <mass value="1.09" />
            <inertia ixx="0.004297" ixy="0" ixz="0" iyy="0.004366" iyz="-0.0008" izz="0.000988" />
        </inertial>
    </link>
    <link name="link5" />
    <joint name="joint5" type="revolute">
        <origin xyz="0 0 0.4" rpy="0 0 0" />
        <parent link="link4_passive" />
        <child link="link5" />
        <axis xyz="0 0 1" />
        <limit lower="-2.879793264" upper="2.879793264" effort="15" velocity="10" />
        <dynamics damping="1" friction="0.1"/>
    </joint>
    <joint name="joint5_passive" type="fixed">
        <origin xyz="0 0 0" rpy="-1.57 0 0" />
        <parent link="link5" />
        <child link="link5_passive" />
    </joint>
    <link name="link5_passive">
        <visual>
            <geometry>
                <mesh filename="package://meshes/link5.stl" />
            </geometry>
            <material name="white"/>
        </visual>
        <collision>
            <geometry>
                <mesh filename="package://meshes/col5.stl" />
            </geometry>
        </collision>
        <inertial>
            <origin xyz=".001262 0.085 0.008" />
            <mass value="2.23" />
            <inertia ixx="0.005744" ixy="-0.00021" ixz="-0.000027" iyy="0.0018" iyz="-0.001278" izz="0.005457" />
        </inertial>
    </link>
    <link name="link6" />
    <joint name="joint6" type="revolute">
        <origin xyz="0 0 0" rpy="0 0 0" />
        <parent link="link5_passive" />
        <child link="link6" />
        <axis xyz="0 0 1" />
        <limit lower="-2.007128639" upper="2.007128639" effort="15" velocity="10" />
        <dynamics damping="1" friction="0.1"/>
    </joint>
    <joint name="joint6_passive" type="fixed">
        <origin xyz="0 0 0" rpy="1.57 0 0" />
        <parent link="link6" />
        <child link="link6_passive" />
    </joint>
    <link name="link6_passive">
        <visual>
            <geometry>
                <mesh filename="package://meshes/link6.stl" />
            </geometry>
            <material name="blue"/>
        </visual>
        <collision>
            <geometry>
                <mesh filename="package://meshes/col6.stl" />
            </geometry>
        </collision>
        <inertial>
            <origin xyz="0 -0.00007 0.010855" />
            <mass value="1.138" />
            <inertia ixx="0.000634" ixy="0" ixz="0" iyy="0.000629" iyz="0" izz="0.000546" />
        </inertial>
    </link>
    <link name="link7" />
    <joint name="joint7" type="revolute">
        <origin xyz="0 0 0" rpy="0 0 0" />
        <parent link="link6_passive" />
        <child link="link7" />
        <axis xyz="0 0 1" />
        <limit lower="-2.617993876" upper="2.617993876" effort="15" velocity="10" />
        <dynamics damping="1" friction="0.1"/>
    </joint>
    <joint name="joint7_passive" type="fixed">
        <origin xyz="0 0 0" rpy="0 0 0" />
        <parent link="link7" />
        <child link="link7_passive" />
    </joint>
    <link name="link7_passive">
        <visual>
            <geometry>
                <mesh filename="package://meshes/link7.stl" />
            </geometry>
            <material name="white"/>
        </visual>
        <collision>
            <geometry>
                <mesh filename="package://meshes/col7.stl" />
            </geometry>
        </collision>
        <inertial>
            <origin xyz="0 0 0.2" />
            <mass value="1.246" />
            <inertia ixx="0.005541" ixy=".000045" ixz="0.000077" iyy="0.004779" iyz="0.000043" izz="0.001723" />
        </inertial>
    </link>

    <joint name="ee_joint" type="fixed">
        <origin xyz="0 0 0.33" rpy="0 0 0" />
        <parent link="link7_passive" />
        <child link="ee" />
    </joint>
    <link name="ee">
        <collision>
            <origin xyz="0 0 -0.075" rpy="0 0 0"/>
            <geometry>
                <box size="0.1 0.1 0.15"/>
            </geometry>
        </collision>
    </link>

</robot>
